@model IEnumerable<PetSuppliesPlus.Model.Store.StoreAdChoiceModel>
    @{
        Layout = null;
    }
    @if (Model != null)
    {
        int counter = 0;
        foreach (var item in Model)
        {
            <tr>
                <td>@(new DateTime(2010,item.AdMonthDetail.Month,1).ToString("MMMM"))</td>
                <td>@item.Storename</td>
                <td>@item.ADOptionName</td>
                <td>@item.Ownername</td>
                <td>@item.IPAddress</td>
                <td>@item.Device</td>
                <td>@item.Browser</td>
                <td>@item.FollowedCorporate</td>
                <td>@item.ChoiceInitials</td>
                <td></td>
            </tr>
            counter++;
        }
        <tr>
            <td colspan="10">
                @Html.Partial("DataPaging")
            </td>
        </tr>
    }
    else
    {
        <tr>
            <td colspan="2">
                <span class="no-data">No store found.</span>
            </td>
        </tr>

    }
